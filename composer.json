{
  "name": "workaholics/ipn-integrator",
  "description": "Adds ipn to a laravel project",
  "type": "composer-plugin",
  "require": {
    "guzzlehttp/guzzle": "^6.0",
    "php": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2@dev",
    "php-coveralls/php-coveralls": "2.2.x-dev",
    "orchestra/testbench": "^6.0@dev"
  },
  "autoload": {
    "psr-4": {
      "Workaholic\\IpnIntegrator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Workaholic\\IpnIntegrator\\Tests\\": "tests"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Isidahomen",
      "email": "bisidahomen@gmail.com"
    }
  ],
  "minimum-stability": "dev"
}
